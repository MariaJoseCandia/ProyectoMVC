@model IEnumerable<Escuela.Core.Curso>
@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <title>Curso</title>
</head>
<body>
    
    <h2>Curso</h2>

    <table>
        <thead>
            <tr> 
                <td>AÃ±o</td> 
                <td>Division</td> 
                <td>Turno</td> 
            </tr>
        </thead>
        <tbody>
            @foreach (Escuela.Core.Curso curso in Model)
            {
                <tr>
                    <td><a asp-controller="Curso" asp-action="Detalle" asp-route-id="@curso.Id">@curso.Anio</a></td>
                    <td><a asp-controller="Curso" asp-action="Detalle" asp-route-id="@curso.Id">@curso.Division</a></td>
                    <td><a asp-controller="Curso" asp-action="Detalle" asp-route-id="@curso.Id">@curso.Turno</a></td>
                </tr>
            }
        </tbody>
    </table>

    <button>
    <a asp-controller="" asp-action="Index">Inicio</a>
    </button>
    
</body>
</html>
